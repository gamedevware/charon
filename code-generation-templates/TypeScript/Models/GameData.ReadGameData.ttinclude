		private readLocalizedText(localizedTextObj: any): <#= context.TargetLanguage.LocalizedTextClassName #> {
			if (typeof (localizedTextObj) !== 'object') {
				return <#= context.TargetLanguage.LocalizedTextClassName #>.empty;
			}

			const textByLanguage = new Map<string, string | null>();
			for (const languageId in localizedTextObj) {
				if (!Object.prototype.hasOwnProperty.call(localizedTextObj, languageId)) {
					continue;
				}
				const text = localizedTextObj[languageId];
				if (!languageId || !text ||
					typeof (languageId) !== 'string' || typeof (text) !== 'string' ||
					languageId === 'notes') {
					continue;
				}
				textByLanguage.set(languageId, text);
			}
			return new <#= context.TargetLanguage.LocalizedTextClassName #>(textByLanguage, this.languageSwitcher);
		}
